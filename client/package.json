  "private": true,
  "dependencies": {
    "antd": "^3.2.2",
    "apollo-cache-inmemory": "1.1.4",
    "apollo-client": "2.2.7",
    "apollo-link-batch-http": "^1.2.3",
    "apollo-link-error": "^1.0.7",
    "apollo-link-http": "^1.5.2",
    "axios": "^0.18.0",
